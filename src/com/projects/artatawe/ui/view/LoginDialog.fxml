<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="125.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projects.artatawe.ui.view.LoginController">
   <children>
      <GridPane layoutX="11.0" layoutY="14.0" prefWidth="296.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="143.0" minWidth="10.0" prefWidth="77.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="219.0" minWidth="10.0" prefWidth="219.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Select User:" />
            <ComboBox fx:id="userComboBox" onInputMethodTextChanged="#handleSelectUser" prefHeight="25.0" prefWidth="217.0" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="loginButton" layoutX="255.0" layoutY="86.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
      <Button layoutX="133.0" layoutY="86.0" mnemonicParsing="false" onAction="#handleRegister" text="Register New User" />
   </children>
</AnchorPane>
